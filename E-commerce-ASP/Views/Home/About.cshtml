    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="bread-inner">
                    <ul class="bread-list">
                        <li><a href="index1.html">@E_commerce_ASP.Resources.Views.Home.About.Home<i class="ti-arrow-right"></i></a></li>
